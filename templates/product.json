            "custom_liquid": "{% render 'product-specs' %}\n\n<div class=\"container d-sm-none d-sm-block\" id=\"especificaciones\">\n  <div class=\"row\">\n  <div class=\"col-md-6 col-12\" style=\"padding-right: 7%;\">\n  {% if product.description != blank %}\n    <h3>Descripción</h3>\n    <div id=\"product-description-container\">\n      <div id=\"product-description\" class=\"short-description\">\n        {{ product.description }}\n      </div>\n    </div>\n    {% if product.description.size > 500 %}\n      <button id=\"toggle-description\" class=\"btn btn-link\">Ver más</button>\n    {% endif %}\n  {% endif %}\n</div>\n\n\n\n        <div class=\"col-md-6 col-12 atributos\">\n  <ul>\n<h3 style=\"padding-top: 10px;\">Acerca del producto</h3>\n\n\n{% if product.metafields.global.materialshopi != blank -%}\n<li><h4><span class=\"generoicon\"></span>Material</h4>\n<p>{{ product.metafields.global.materialshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.tipodeplato != blank -%}\n<li><h4><span class=\"generoicon\"></span>Tipo de plato</h4>\n<p>{{ product.metafields.global.tipodeplato| metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.tubelessshopi != blank -%}\n<li><h4><span class=\"generoicon\"></span>Tubeless</h4>\n<p>{{ product.metafields.global.tubelessshopi| metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n\n{% if product.metafields.global.anchodelproductoshopi != blank -%}\n<li><h4><span class=\"generoicon\"></span>Ancho del producto</h4>\n<p>{{ product.metafields.global.anchodelproductoshopi| metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n\n{% if product.metafields.global.cantidad_de_huecos != blank -%}\n<li><h4><span class=\"generoicon\"></span>Cantidad de huecos</h4>\n<p>{{ product.metafields.global.cantidad_de_huecos| metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.marco != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Marco</h4>\n<p>{{ product.metafields.custom.marco | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n\n{% if product.metafields.global.tipo_de_frenoshopi != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Tipo de Freno</h4>\n<p>{{ product.metafields.global.tipo_de_frenoshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.velocidades != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Velocidades</h4>\n<p>{{ product.metafields.global.velocidades | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.guantesldedo != blank -%}\n<li><h4><span class=\"marcaicon\"></span>Longitud Dedo</h4>\n<p>{{ product.metafields.global.guantesldedo | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.generoshopi != blank -%}\n<li><h4><span class=\"marcaicon\"></span>Genero</h4>\n<p>{{ product.metafields.global.generoshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n\n{% if product.metafields.global.espigo != blank -%}\n<li><h4><span class=\"climaicon\"></span>Espigo</h4>\n{% for espigo in product.global.espigo.value %}\n<p>{{ espigo}}\n{% endfor %}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.desbloqueo != blank -%}\n<li><h4><span class=\"usoicon\"></span>Desbloqueo</h4>\n{% for desbloqueo in product.metafields.global.desbloqueo.value %}\n<p>{{ desbloqueo }}\n{% endfor %}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.consistenciashopi!= blank -%}\n<li><h4><span class=\"consetracionicon\"></span>Consistencia</h4>\n<p>{{ product.metafields.global.consistenciashopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.compatible_monitor_de_ritmo_cardiaco!= blank -%}\n<li><h4><span class=\"anoicon\"></span>Compatible Monitor Cardiaco</h4>\n<p>{{ product.metafields.global.compatible_monitor_de_ritmo_cardiaco | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.color_del_lente != blank -%}\n<li><h4><span class=\"paisicon\"></span>Color del Lente</h4>\n<p>{{ product.metafields.global.color_del_lente | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.caracteristicas_del_material!= blank -%}\n<li><h4><span class=\"perfumistaicon\"></span>Caracteristicas del Material</h4>\n<p>{{ product.metafields.global.caracteristicas_del_material | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.capacidadshopi != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Capacidad</h4>\n<p>{{ product.metafields.global.capacidadshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.manillar != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Manillar</h4>\n<p>{{ product.metafields.custom.manillar | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.tenedor != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Tenedor</h4>\n<p>{{ product.metafields.custom.tenedor | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.bater_a != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Batería</h4>\n<p>{{ product.metafields.custom.bater_a | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.cassette != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Cassette</h4>\n<p>{{ product.metafields.custom.cassette | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.llantas != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Llantas</h4>\n<p>{{ product.metafields.custom.llantas | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.suspensi_n != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Suspensión</h4>\n<p>{{ product.metafields.custom.suspensi_n | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.tamano_de_rinshopi != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Tamaño de Rin</h4>\n<p>{{ product.metafields.global.tamano_de_rinshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.motor != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Motor</h4>\n<p>{{ product.metafields.custom.motor | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.custom.sensor != blank -%}\n<li><h4><span class=\"notas-salidaicon\"></span>Sensor</h4>\n<p>{{ product.metafields.custom.sensor | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n\n {% if product.metafields.global.bluetooth != blank -%}\n<li><h4><span class=\"notas-corazonicon\"></span>Bluetooth</h4>\n<p>{{ product.metafields.global.bluetooth | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n {% if product.metafields.global.tensor != blank -%}\n<li><h4><span class=\"notas-corazonicon\"></span>Tensor</h4>\n<p>{{ product.metafields.global.tensor | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.funciones_adicionalesshopi != blank -%}\n<li><h4><span class=\"notas-corazonicon\"></span>Funciones adcionales</h4>\n<p>{{ product.metafields.global.funciones_adicionalesshopi | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n\n{% if product.metafields.global.cadencia!= blank -%}\n<li><h4><span class=\"notas-baseicon\"></span>Cadencia</h4>\n<p>{{ product.metafields.global.cadencia | metafield_text }}\n{%- else -%}\n{%- endif %}\n</p>\n</li>\n</ul>\n</div>\n  </div>\n</div>\n\n\n\n\n\n\n\n\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function () {\n    const descriptionContainer = document.getElementById(\"product-description-container\");\n    const descriptionElement = document.getElementById(\"product-description\");\n    const toggleButton = document.getElementById(\"toggle-description\");\n\n    if (descriptionElement && toggleButton) {\n      // Guardar el contenido original del HTML\n      const fullDescription = descriptionElement.innerHTML;\n      const shortDescription = fullDescription.substring(0, 500) + \"...\";\n\n      // Establecer estado inicial\n      descriptionElement.innerHTML = shortDescription;\n\n      // Alternar entre descripción completa y truncada\n      toggleButton.addEventListener(\"click\", function () {\n        const isShort = descriptionElement.classList.contains(\"short-description\");\n        if (isShort) {\n          descriptionElement.innerHTML = fullDescription;\n          toggleButton.textContent = \"Ver menos\";\n        } else {\n          descriptionElement.innerHTML = shortDescription;\n          toggleButton.textContent = \"Ver más\";\n        }\n        descriptionElement.classList.toggle(\"short-description\");\n      });\n    }\n  });\n</script>\n\n<style>\n  .short-description {\n    overflow: hidden;\n    text-overflow: ellipsis;\n    display: -webkit-box;\n    -webkit-line-clamp: 6; /* Cambia el número de líneas visibles */\n    -webkit-box-orient: vertical;\n  }\n.d-sm-block{\ndisplay: block;\n}\n.d-xs-none{\ndisplay: none;\n}\n@media only screen and (max-width: 600px) {\n.d-sm-none{\ndisplay: none;\n}\n.d-xs-block{\ndisplay:block;\n}\n}\n</style>",
